{
  "name": "QuillBot Premium Cracker - By RedHydra",
  "version": "3.1.0",
  "description": "Enhanced QuillBot Cracker with Beautiful Futuristic Cyberpunk Theme - Redesigned by RedHydra",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
    "default_title": "QuillBot Premium Unlocker",
    "default_icon": {
      "16": "images/logo-16.png",
      "48": "images/logo-48.png",
      "128": "images/logo-128.png"
    }
  },
  "web_accessible_resources": [
    {
      "resources": [
        "quillbot.js",
        "inject.js",
        "futuristic-theme.css"
      ],
      "matches": [
        "https://quillbot.com/*"
      ]
    }
  ],
  "content_scripts": [
    {
      "js": [
        "inject.js"
      ],
      "matches": [
        "https://quillbot.com/*"
      ],
      "all_frames": true,
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "cookies",
    "storage",
    "tabs"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "icons": {
    "16": "images/logo-16.png",
    "48": "images/logo-48.png",
    "128": "images/logo-128.png"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
}